    "import msoffcrypto\n",
    "import io\n",
    "import pandas as pd\n",
    "import scipy.stats as stats\n",
    "\n",
    "filename = '(Locked) Copy of covid_19 final master sheet12-12-2020 (1).xlsx'\n",
    "file = msoffcrypto.OfficeFile(open(filename, \"rb\"))\n",
    "file.load_key(password=\"1234\")\n",
    "\n",
    "decrypted = io.BytesIO()\n",
    "file.decrypt(decrypted)\n",
    "\n",
    "df_master = pd.read_excel(decrypted)\n",
    "df_master\n",
    "\n",
    "#%pwd\n"
   ]
