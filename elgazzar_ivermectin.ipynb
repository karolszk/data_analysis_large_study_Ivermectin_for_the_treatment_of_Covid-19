    "def print_table1(s,label):\n",
    "    #print(s.describe())\n",
    "    print(\"{} N={} {:.1f}±{:.1f} range {:.0f}-{:.0f}\".format(\n",
    "        label, s.count(), s.mean(), s.std(), s.min(), s.max()))\n",
    "\n",
    "def fun(x):\n",
    "    if \"%\" in str(x):\n",
    "        return float(str(x).strip().replace(\"%\",\"\").replace(\",\",\".\")) / 100.0\n",
    "    return x\n",
    "\n",
    "def calculate_table1(v, m):\n",
    "    assert v.isna().sum() == 0\n",
    "    v = v.astype(str)\n",
    "    v=v.str.strip().replace(r'.oo%','%',regex=True)\n",
    "    v=v.str.replace(r'oo',r'00',regex = True)\n",
    "    v=v.str.replace(r'o',r'0',regex = True)\n",
    "    v=v.str.replace(r'[\\']','',regex=True)\n",
    "    \n",
    "    v = v.apply(fun)\n",
    "\n",
    "    v = pd.to_numeric(v) * m\n",
    "    return v"
   ]
